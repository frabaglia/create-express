    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "flow-bin": "^0.53.0",
    "flow-typed": "^2.1.5",
    "nodemon": "^1.11.0"
  }
